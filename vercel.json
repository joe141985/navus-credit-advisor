{
  "functions": {
    "chat-frontend/index.html": {
      "runtime": "@vercel/static"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/chat-frontend/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/chat-frontend/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}